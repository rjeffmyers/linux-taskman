#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_install:
	install -D -m 755 taskmanager.py debian/linux-taskman/usr/bin/linux-taskman
	install -D -m 644 taskmanager.desktop debian/linux-taskman/usr/share/applications/linux-taskman.desktop
	install -D -m 644 README.md debian/linux-taskman/usr/share/doc/linux-taskman/README.md